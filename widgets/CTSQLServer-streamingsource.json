{
  "metadata": {
    "spec-version": "1.3"
  },
  "configuration-groups": [
    {
      "label": "CT SQL Server Configuration",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Reference Name",
          "name": "referenceName"
        },
        {
          "widget-type": "textbox",
          "label": "Hostname",
          "name": "hostname"
        },
        {
          "widget-type": "textbox",
          "label": "Port",
          "name": "port",
          "widget-attributes": {
            "default": "1433"
          }
        },
        {
          "widget-type": "textbox",
          "label": "Username",
          "name": "username"
        },
        {
          "widget-type": "password",
          "label": "Password",
          "name": "password"
        },
        {
          "widget-type": "textbox",
          "label": "Database name",
          "name": "dbname"
        }
      ]
    }
  ],
  "outputs": [
    {
      "widget-type": "non-editable-schema-editor",
      "schema": {
        "name": "etlSchemaBody",
        "type": "record",
        "fields": [
          {
            "name": "message",
            "type": [
              "bytes",
              "null"
            ]
          }
        ]
      }
    }
  ]
}
